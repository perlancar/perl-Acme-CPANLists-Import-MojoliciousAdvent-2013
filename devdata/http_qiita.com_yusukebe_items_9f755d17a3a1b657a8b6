<span class="k">use</span> <span class="nn">Mojo::</span><span class="n">Base</span> <span class="s">'Mojolicious'</span><span class="p">;</span>

<span class="k">sub </span><span class="nf">startup</span> <span class="p">{</span>
    <span class="k">my</span> <span class="nv">$self</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span>
    <span class="k">my</span> <span class="nv">$r</span> <span class="o">=</span> <span class="nv">$self</span><span class="o">-&gt;</span><span class="n">routes</span><span class="p">;</span>
    <span class="k">my</span> <span class="nv">$logged_in</span> <span class="o">=</span> <span class="nv">$r</span><span class="o">-&gt;</span><span class="n">bridge</span><span class="o">-&gt;</span><span class="n">to</span><span class="p">(</span><span class="s">'root#login'</span><span class="p">);</span>
