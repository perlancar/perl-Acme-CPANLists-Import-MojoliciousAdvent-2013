<span class="nb">package</span> <span class="nn">MyApp::</span><span class="n">Root</span><span class="p">;</span>
<span class="k">use</span> <span class="nn">Mojo::</span><span class="n">Base</span> <span class="s">'Mojolicious::Controller'</span><span class="p">;</span>

<span class="k">sub </span><span class="nf">index</span> <span class="p">{</span>
    <span class="k">my</span> <span class="nv">$self</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span>
    <span class="nv">$self</span><span class="o">-&gt;</span><span class="n">render</span><span class="p">(</span><span class="n">text</span> <span class="o">=&gt;</span> <span class="s">'You are logged-in!'</span><span class="p">);</span>
<span class="p">}</span>

<span class="k">sub </span><span class="nf">login</span> <span class="p">{</span>
    <span class="k">my</span> <span class="nv">$self</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span>
    <span class="k">my</span> <span class="nv">$password</span> <span class="o">=</span> <span class="nv">$self</span><span class="o">-&gt;</span><span class="n">param</span><span class="p">(</span><span class="s">'password'</span><span class="p">);</span>
    <span class="k">return</span> <span class="mi">1</span> <span class="k">if</span> <span class="nv">$password</span> <span class="o">&amp;&amp;</span> <span class="nv">$password</span> <span class="ow">eq</span> <span class="s">'password'</span><span class="p">;</span>
    <span class="nv">$self</span><span class="o">-&gt;</span><span class="n">render</span><span class="p">(</span> <span class="n">text</span> <span class="o">=&gt;</span> <span class="s">'Your password is "password"'</span> <span class="p">);</span>
    <span class="k">return</span> <span class="nb">undef</span><span class="p">;</span>
<span class="p">}</span>

<span class="mi">1</span><span class="p">;</span>
</pre></div>
