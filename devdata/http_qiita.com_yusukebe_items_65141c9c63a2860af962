    <span class="k">my</span> <span class="nv">$self</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span>
    <span class="k">my</span> <span class="nv">$id</span> <span class="o">=</span> <span class="nv">$self</span><span class="o">-&gt;</span><span class="n">stash</span><span class="o">-&gt;</span><span class="p">{</span><span class="n">id</span><span class="p">};</span>
    <span class="nv">$self</span><span class="o">-&gt;</span><span class="n">render</span><span class="p">(</span> <span class="n">text</span> <span class="o">=&gt;</span> <span class="s">"Entry ID is $id!"</span> <span class="p">);</span>
