package MojibakeApp::Example;
use Mojo::Base &#39;Mojolicious::Controller&#39;;
use Encode &#39;decode&#39;;

# This action will render a template
sub welcome {
  my $self = shift;

my $moji = {hello =&gt; &quot;æå­åã&quot;};
my $json = $self-&gt;render(
json =&gt; $moji , partial =&gt; 1);

$self-&gt;app-&gt;log-&gt;debug($json);
print Dumper($json);

$self-&gt;render(
   message =&gt; $json);

```

