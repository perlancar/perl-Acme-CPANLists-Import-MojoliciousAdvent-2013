<span class="nb">package</span> <span class="nn">MyApp::Logic::</span><span class="n">SomeThing</span><span class="p">;</span>
<span class="k">use</span> <span class="n">Mouse</span><span class="p">;</span>
<span class="k">use</span> <span class="nn">Mojo::</span><span class="n">Home</span><span class="p">;</span>
<span class="k">use</span> <span class="n">Cwd</span> <span class="sx">qw/abs_path/</span><span class="p">;</span>

<span class="k">sub </span><span class="nf">project_dir</span> <span class="p">{</span>
    <span class="k">my</span> <span class="nv">$self</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span>
    <span class="k">my</span> <span class="nv">$home</span> <span class="o">=</span> <span class="nn">Mojo::</span><span class="n">Home</span><span class="o">-&gt;</span><span class="k">new</span><span class="p">;</span>
    <span class="nv">$home</span><span class="o">-&gt;</span><span class="n">detect</span><span class="p">(</span> <span class="nb">ref</span> <span class="nv">$self</span> <span class="p">);</span>
    <span class="k">my</span> <span class="nv">$project_dir</span> <span class="o">=</span> <span class="nv">$home</span><span class="o">-&gt;</span><span class="n">rel_dir</span><span class="p">(</span><span class="s">'.'</span><span class="p">);</span>
    <span class="k">return</span> <span class="nv">$project_dir</span><span class="p">;</span>
<span class="p">}</span>

