<div class="highlight"><pre>
<span class="nb">package</span> <span class="nn">MyApp::Web::</span><span class="n">Example</span><span class="p">;</span>
<span class="k">use</span> <span class="nn">Mojo::</span><span class="n">Base</span> <span class="s">'Mojolicious::Controller'</span><span class="p">;</span>

<span class="k">sub </span><span class="nf">welcome</span> <span class="p">{</span>
    <span class="k">my</span> <span class="nv">$self</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span>
    <span class="k">my</span> <span class="nv">$log_dir</span> <span class="o">=</span> <span class="nv">$self</span><span class="o">-&gt;</span><span class="n">app</span><span class="o">-&gt;</span><span class="n">home</span><span class="o">-&gt;</span><span class="n">rel_dir</span><span class="p">(</span><span class="s">'log'</span><span class="p">);</span>
