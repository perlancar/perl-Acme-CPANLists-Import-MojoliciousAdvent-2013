        <span class="n">korpokkur</span> <span class="o">=&gt;</span> <span class="p">{</span> <span class="nv">%</span><span class="p">{</span><span class="nv">$config</span><span class="p">},</span> <span class="n">sitemap</span> <span class="o">=&gt;</span> <span class="nv">$sitemap</span><span class="p">,</span> <span class="p">},</span>
