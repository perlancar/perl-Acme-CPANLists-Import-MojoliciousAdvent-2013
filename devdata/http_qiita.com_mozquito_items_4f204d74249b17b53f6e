    <span class="k">my</span> <span class="nv">$sitemap</span> <span class="o">=</span> <span class="nn">YAML::Syck::</span><span class="n">LoadFile</span><span class="p">(</span> <span class="nv">$app</span><span class="o">-&gt;</span><span class="n">home</span><span class="o">-&gt;</span><span class="n">rel_file</span><span class="p">(</span><span class="s">'./sitemap.yaml'</span><span class="p">)</span> <span class="p">);</span>

