    <span class="n">enable</span> <span class="s">"Auth::Basic"</span><span class="p">,</span> <span class="n">authenticator</span> <span class="o">=&gt;</span> <span class="o">\&amp;</span><span class="n">authen_cb</span><span class="p">;</span> 
    <span class="n">app</span><span class="o">-&gt;</span><span class="n">start</span><span class="p">;</span>
